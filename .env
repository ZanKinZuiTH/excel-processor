# Environment
ENVIRONMENT=production

# Security
SECRET_KEY=your-secret-key-here
JWT_SECRET_KEY=your-jwt-secret-key-here
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# Database
DB_USER=excel_processor
DB_PASSWORD=your-secure-password-here
DB_NAME=excel_processor_db
DATABASE_URL=postgresql://${DB_USER}:${DB_PASSWORD}@db:5432/${DB_NAME}

# Redis
REDIS_URL=redis://redis:6379/0

# CORS
ALLOWED_ORIGINS=https://ui.example.com,https://api.example.com

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# DICOM Settings
DICOM_STORAGE_PATH=/app/dicom_files
MAX_DICOM_FILE_SIZE=100MB

# SSL/TLS
SSL_CERT_PATH=/app/certs/cert.pem
SSL_KEY_PATH=/app/certs/key.pem

# ตั้งค่าพื้นฐาน
APP_NAME=ระบบจัดการเทมเพลต Excel
APP_VERSION=1.0.0
DEBUG=False

# ตั้งค่าความปลอดภัย
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# ตั้งค่าฐานข้อมูล
DATABASE_URL=sqlite:///./excel_data.db

# ตั้งค่า logging
LOG_FILE=app.log 